{
  "name": "elm-webpack-seed",
  "version": "0.0.1",
  "description": "a starting point for elm projects",
  "scripts": {
    "start": "elm-live src/Main.elm --open --path-to-elm-make=node_modules/.bin/elm-make -- --output app.js",
    "dev": "yarn start --debug",
    "build": "elm-make src/Main.elm --output=app.js"
  },
  "keywords": [
    "elm"
  ],
  "author": "Max Burri",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-live": "^2.7.5"
  }
}
