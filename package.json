{
  "name": "elm-webpack-seed",
  "version": "0.0.1",
  "description": "a starting point for elm projects",
  "scripts": {
    "clean": "rimraf ./dist",
    "copy-assets": "copy index.html ./dist && copy public/* ./dist/",
    "start": "elm-live src/Main.elm --open --path-to-elm-make='node_modules/.bin/elm make' -- --output app.js",
    "dev": "npm start --debug",
    "build": "npm run clean && npm run copy-assets && node_modules/.bin/elm make src/Main.elm --output=dist/app.js --optimize",
    "deploy": "npm run build && gh-pages -d dist/"
  },
  "keywords": [
    "elm"
  ],
  "author": "Max Burri",
  "license": "ISC",
  "devDependencies": {
    "copy": "^0.3.2",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.3",
    "elm-live": "^4.0.2",
    "gh-pages": "^2.2.0",
    "prettier": "2.0.4"
  }
}
