{
  "name": "elm-webpack-seed",
  "version": "0.0.1",
  "description": "a starting point for elm projects",
  "scripts": {
    "start": "elm-live src/Main.elm --open --path-to-elm-make=node_modules/.bin/elm make -- --output app.js",
    "dev": "npm start --debug",
    "build": "copy index.html ./dist  && node_modules/.bin/elm make src/Main.elm --output=dist/app.js",
    "deploy": "npm run build && gh-pages -d dist/"
  },
  "keywords": [
    "elm"
  ],
  "author": "Max Burri",
  "license": "ISC",
  "devDependencies": {
    "copy": "^0.3.2",
    "elm": "^0.19.0-bugfix2",
    "elm-format": "^0.8.1",
    "elm-live": "^3.2.3",
    "gh-pages": "^2.0.1"
  }
}